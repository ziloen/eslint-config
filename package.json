{
  "name": "@ziloen/eslint-config",
  "version": "0.1.37",
  "author": "ziloen",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "inspect": "pnpm dlx @eslint/config-inspector@latest",
    "prepack": "tsup --minify",
    "release": "bumpp --commit --no-push && pnpm publish",
    "lint": "cross-env TIMING=50 eslint ."
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@eslint/js": "^9.6.0",
    "@stylistic/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "8.0.0-alpha.34",
    "eslint": "^9.6.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-unicorn": "^54.0.0",
    "eslint-plugin-vue": "^9.26.0",
    "eslint-plugin-ziloen": "^0.1.12",
    "typescript": "^5.5.2",
    "typescript-eslint": "8.0.0-alpha.34",
    "vue-eslint-parser": "^9.4.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "bumpp": "^9.4.1",
    "cross-env": "^7.0.3",
    "react": "^18.3.1",
    "tsup": "^8.1.0",
    "vue": "^3.4.31"
  }
}
