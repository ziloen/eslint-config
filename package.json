{
  "name": "@ziloen/eslint-config",
  "version": "0.1.68",
  "author": "ziloen",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "inspect": "pnpm dlx @eslint/config-inspector@latest",
    "prepack": "tsdown --minify",
    "release": "bumpp --commit --no-push && pnpm publish",
    "lint": "cross-env TIMING=50 eslint ."
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@eslint-react/eslint-plugin": "^1.52.3",
    "@eslint/js": "^9.32.0",
    "@stylistic/eslint-plugin": "^5.2.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-regexp": "^2.9.1",
    "eslint-plugin-unicorn": "^60.0.0",
    "eslint-plugin-vue": "^10.4.0",
    "eslint-plugin-ziloen": "^0.1.16",
    "typescript": "^5.9.2",
    "typescript-eslint": "8.39.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "bumpp": "^10.2.2",
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "react": "^19.1.1",
    "tsdown": "0.13.3",
    "vue": "^3.5.18"
  },
  "peerDependencies": {
    "eslint": "^9"
  }
}