{
  "name": "@ziloen/eslint-config-monorepo",
  "version": "0.0.63",
  "private": true,
  "author": "ziloen",
  "license": "MIT",
  "scripts": {
    "release": "bumpp package.json packages/*/package.json --commit --no-push && pnpm -r publish --access public"
  },
  "packageManager": "pnpm@7.13.0",
  "dependencies": {
    "@ziloen/eslint-config": "workspace:*",
    "@ziloen/eslint-config-basic": "workspace:*",
    "@ziloen/eslint-config-react": "workspace:*",
    "@ziloen/eslint-config-typescript": "workspace:*",
    "@ziloen/eslint-config-vue": "workspace:*",
    "@ziloen/eslint-config-format": "workspace:*",
    "bumpp": "latest",
    "eslint": "latest",
    "react": "^18.2.0",
    "typescript": "latest"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.6",
    "@types/node": "^18.8.1",
    "@types/react": "^18.0.21"
  },
  "eslintConfig": {
    "extends": [
      "@ziloen",
      "@ziloen/eslint-config-format"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  }
}